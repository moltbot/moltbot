{
  "name": "@openclaw/fish-speech",
  "version": "1.0.0",
  "description": "Fish-Speech TTS provider plugin for OpenClaw with voice cloning support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openclaw",
    "tts",
    "fish-speech",
    "text-to-speech",
    "voice-cloning"
  ],
  "author": "OpenClaw Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openclaw/openclaw.git",
    "directory": "extensions/fish-speech"
  },
  "peerDependencies": {
    "openclaw": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
